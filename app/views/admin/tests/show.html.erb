<h1>List of questions for test #<%= @test.id %> - "<%= @test.title %>"</h1>

<table>
  <tr>
    <th>Question text</th>
    <th colspan="3">Actions</th>
  </tr>
  <%= render partial: "admin/questions/question", collection: @test.questions %>
</table>

<br>
<%= link_to 'Back', admin_tests_path %> |
<%= link_to 'Edit', edit_admin_test_path(@test) %> |
<%= link_to 'Add new question', new_admin_test_question_path(@test) %>
